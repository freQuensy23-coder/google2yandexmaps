{
  "manifest_version": 2,
  "name": "Google to Yandex Maps Redirect",
  "version": "1.0",
  "description": "Автоматически подменяет ссылку с Google Карт на Яндекс Карты",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon48.png",
    "default_title": "Google to Yandex Maps Redirect"
  }
}
